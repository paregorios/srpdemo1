               <placeName xml:id="name9-2" xml:lang="en" source="#bib9-1">ANAZARBOS</placeName>
               <placeName xml:id="name9-3" xml:lang="en" source="#bib9-1">Anazarba</placeName>
               <placeName xml:id="name9-4" xml:lang="syr" syriaca-tags="#syriaca-authorized">ܥܝܢܕܐܒܐ ܐܘ ܙܢܐܙܪܒܐ</placeName>
               <placeName xml:id="name9-5"
                          xml:lang="syr-Syrj"
                          source="#bib9-2"
                          corresp="#name9-6 #name9-7">ܥܰܝܢܕܰܐܒܐ ܐܰܘܰ ܙܢܐܙܪܒܐ</placeName>
               <placeName xml:id="name9-6"
                          xml:lang="ar"
                          source="#bib9-3"
                          corresp="#name9-5 #name9-7">عين زربة، انازربة</placeName>
               <placeName xml:id="name9-7"
                          xml:lang="en"
                          source="#bib9-4"
                          corresp="#name9-5 #name9-6">ʿAyn Zarba (Anāzarba)</placeName>
               <desc xml:id="abstract-en-9"/>
               <desc xml:lang="syr-Syrj">
                  <quote source="#bib9-2">ܡܕܝܢܬܳܐ ܕܰܒܩܝܠܺܝܩܝܰܐ ܣܺܝܡܳܐ ܗܘܳܬ ܥܰܠ ܢܗܰܪ ܓܺܝ̣ܚܘܢ ܘܝܰܘܡܳܢ ܗܘܳܬ ܩܪܺܝܬܳܐ ܕܡܶܬܶܐܡܰܪ ܠܳܗ̇ ܐܰܢܐܙܘܪܐ.</quote>
               </desc>
               <desc xml:lang="ar">
                  <quote source="#bib9-3">مدينة في قيليقية كانت واقعة على نهر جيحان وامست الان قرية يقال لها انازروه.</quote>
               </desc>
               <desc xml:lang="en">
                  <quote source="#bib9-4">a town in Cilicia, on the river Jījān. Today it is a small village called Anāzūra.</quote>
               </desc>
               <location type="gps" source="#bib9-1">
                  <geo>37.263889 35.905556</geo>
               </location>
               <state type="existence"/>
               <idno type="URI">http://syriaca.org/place/9</idno>
               <idno type="URI">http://pleiades.stoa.org/places/658378</idno>
               <idno type="URI">http://en.wikipedia.org/wiki/Anazarbus</idno>
               <bibl xml:id="bib9-1">
                  <title xml:lang="en">The Gorgias Encyclopedic Dictionary of the Syriac Heritage</title>
                  <ptr target="http://syriaca.org/bibl/1"/>
                  <citedRange unit="maps">Map I A1, II A1</citedRange>
               </bibl>
               <bibl xml:id="bib9-2">
                  <title xml:lang="syr">ܒܪ̈ܘܠܐ ܒܕܝܪ̈ܐ ܕܥܠ ܡܪܕܘܬ ܝܘܠܦܢ̈ܐ ܣܘܪ̈ܝܝܐ ܗܕܝܪ̈ܐ</title>
                  <ptr target="http://syriaca.org/bibl/3"/>
